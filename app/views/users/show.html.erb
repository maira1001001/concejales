<div class="page-header">
  <%= link_to users_path, class: 'btn btn-default' do %>
    <%= icon 'list-alt' %>
    <%= t 'global.actions.back' %>
  <% end %>
  <h1><%= t "users.new.title" %></h1>


  <dl class="dl-horizontal">
    <dt>Email:</dt>
    <dd><%= @user.email %></dd>

    <dt>Nombre y Apellido:</dt>
    <dd><%= @user.person %></dd>

    <dt>Tipo de Usuario:</dt>
    <dd><%= t_enum @user, :roles %></dd>
  </dl>

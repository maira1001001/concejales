<div class="page-header">
  <div class="row">
    <div class="col-md-5">
      <h1><%= t '.title', default: t('global.edit.title') %></h1>
    </div>
    <div class="col-md-7">
      <div class="header-actions">
        <%= link_to icon('chevron-left') personal_informations_path, class: 'btn btn-default', data: { toggle: 'tooltip', container: 'body' }, title: t('.back', default: t('global.actions.back')) %>
        <%= link_to icon('eye-open'), @personal_information, class: 'btn btn-default', data: { toggle: 'tooltip', container: 'body' }, title: t('.show', default: t('global.actions.show')) %>
      </div>
    </div>
  </div>
</div>

<%= render 'form' %>
